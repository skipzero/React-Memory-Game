{
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxparams": 4,
  "maxdepth": 3,
  "maxstatements": 100,
  "maxcomplexity": 10,
  "maxlen": 120,

  "globalstrict": true,
  "laxcomma": true,

  "browser": true
}